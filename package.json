{
  "_comment": "Website using Jekyll and Vue",
  "name": "labs-vue-jekyll",
  "repository": "https://github.com/pegalabs/labs",
  "version": "0.1.0",
  "description": "A bare-bones template for building static sites that leverage a modern javascript framework for dynamic functionality. Built with [NPM](https://www.npmjs.com), [Vue](https://vuejs.org), and [Jekyll](https://jekyllrb.com).",
  "main": "main.js",
  "scripts": {
    "packit": "webpack --watch",
    "buildit": "bundle exec jekyll build --watch --incremental",
    "jekyllfy": "npm run packit & npm run buildit & npm run serve",
    "serve": "browser-sync start --config browser-sync.js"
  },
  "keywords": [
    "jekyll",
    "Vue"
  ],
  "author": "Bala Raman",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.23.6",
    "extract-text-webpack-plugin": "^2.1.0",
    "json-loader": "^0.5.7",
    "marked": "^0.3.19",
    "css-loader": "0.28.11",
    "vue": "^2.5.16",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.16",
    "webfontloader": "^1.6.28",
    "webpack": "^2.2.1"
  }
}
